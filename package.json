{
	"name": "NodeExpressApi",
	"version": "1.0.0",
	"description": "Express API Server",
	"author": "",
	"license": "ISC",
	"engines": {
		"node": ">= 14 < 21"
	},
	"scripts": {
		"start": "cross-env NODE_ENV=production node dist/server.js",
		"start:local": "npm run build && cross-env NODE_ENV=development node dist/server.js",
		"dev": "cross-env NODE_ENV=development nodemon --exec 'node --inspect=0.0.0.0:9229 -r ts-node/register -r tsconfig-paths/register' src/server.ts",
		"build:dev": "npm run clean && npm run prettier && swc src -d dist --source-maps --copy-files",
		"build": "npm run clean && npm run prettier && npm run build:tsc && swc src -d dist --source-maps --copy-files",
		"build:tsc": "npm run prettier && tsc && tsc-alias",
		"test": "glob -c \"node --trace-deprecation --no-warnings --test --require ts-node/register -r tsconfig-paths/register\" \"./src/test/**/*.test.ts\"",
		"run:test": "./run-tests.sh",
		"lint": "eslint --ignore-path .gitignore --ext .ts src",
		"lint:fix": "npm run lint -- --fix",
		"deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
		"deploy:dev": "pm2 start ecosystem.config.js --only dev",
		"prettier": "prettier --config .prettierrc --write .",
		"clean": "rimraf dist",
		"prepare": "husky",
		"npm:outdated": "npm outdated",
		"npm:latest-package-update": "npx npm-check-updates -u"
	},
	"dependencies": {
		"@types/amqp-connection-manager": "^3.4.5",
		"@types/amqplib": "^0.10.7",
		"@types/opossum": "^8.1.8",
		"@types/redis": "^4.0.11",
		"amqp-connection-manager": "^4.1.14",
		"amqplib": "^0.10.5",
		"axios": "^1.8.3",
		"bcrypt": "^5.1.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"class-validator-jsonschema": "^5.0.2",
		"cls-hooked": "^4.2.2",
		"compression": "^1.8.0",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^16.4.7",
		"envalid": "^8.0.0",
		"express": "^4.21.2",
		"express-actuator": "^1.8.4",
		"express-jwt": "^8.5.1",
		"express-rate-limit": "^7.5.0",
		"guid-typescript": "^1.0.9",
		"helmet": "^8.0.0",
		"hpp": "^0.2.3",
		"http-status-codes": "^2.3.0",
		"jsonwebtoken": "^9.0.2",
		"linq": "^3.2.4",
		"mediatr-ts": "^2.0.1",
		"morgan": "^1.10.0",
		"neverthrow": "^8.2.0",
		"opossum": "^8.4.0",
		"redis": "^4.7.0",
		"reflect-metadata": "^0.2.2",
		"routing-controllers": "^0.11.2",
		"routing-controllers-openapi": "^5.0.0",
		"swagger-ui-express": "^5.0.1",
		"typedi": "^0.10.0",
		"typeorm": "^0.3.21",
		"uuid": "^11.1.0",
		"winston": "^3.17.0",
		"winston-daily-rotate-file": "^5.0.0"
	},
	"devDependencies": {
		"@faker-js/faker": "^9.6.0",
		"@swc/cli": "^0.6.0",
		"@swc/core": "^1.11.9",
		"@types/autocannon": "^7.12.6",
		"@types/bcrypt": "^5.0.2",
		"@types/compression": "^1.7.5",
		"@types/cookie-parser": "^1.4.8",
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.0",
		"@types/express-jwt": "^7.4.4",
		"@types/glob": "^8.1.0",
		"@types/hpp": "^0.2.6",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/morgan": "^1.9.9",
		"@types/node": "^22.13.10",
		"@types/sinon": "^17.0.4",
		"@types/supertest": "^6.0.2",
		"@types/swagger-ui-express": "^4.1.8",
		"@typescript-eslint/eslint-plugin": "^8.26.1",
		"@typescript-eslint/parser": "^8.26.1",
		"autocannon": "^8.0.0",
		"cross-env": "^7.0.3",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.1.1",
